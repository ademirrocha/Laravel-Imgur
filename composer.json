{
    "name": "arf/imgur",
    "description": "Imgur upload API with Laravel.",
    "keywords": ["laravel", "upload", "imgur", "images"],
    "homepage": "https://github.com/ademirrocha/Laravel-Imgur",
    "type": "library",
    "authors": [
        {
            "name": "Yish",
            "email": "mombuartworks@gmail.com"
        },
        {
            "name": "Ademir Rocha",
            "email": "tiademir.rocha93@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Arf\\Imgur\\": "src/"
        }
    },
    "extra": {
        "component": "package",
        "laravel": {
            "providers": [
                "Arf\\Imgur\\UploadServiceProvider"
            ],
            "aliases": {
                "Imgur": "Arf\\Imgur\\Facades\\Upload"
            }
        }
    }
}
